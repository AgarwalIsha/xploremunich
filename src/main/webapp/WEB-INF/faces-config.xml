<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

<application>
  <resource-bundle>
   <base-name>com.indianguide.global</base-name>
   <var>msg</var>
  </resource-bundle>
 </application>
<application>
        <message-bundle>com.indianguide.global</message-bundle>
        <locale-config>
            <default-locale>en_US</default-locale>
            <supported-locale>en_US</supported-locale>
        </locale-config>
    </application>
	<navigation-rule>
	<from-view-id>/</from-view-id>
		<navigation-case>
			<display-name>homePage</display-name>
			<from-outcome>homePage</from-outcome>
			<to-view-id>/pages/home/homePage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>